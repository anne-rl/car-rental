        <name>onchange start holiday validation</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
   //Type appropriate comment here, and begin script below

	var holiday = new GlideAjax("CarRentalUtils");
	holiday.addParam("sysparm_name", "isDateOnHoliday");
	holiday.addParam("sysparm_dateTime", newValue);
	
	holiday.getXML(isDateOnHoliday);
	
	function isDateOnHoliday(response){
			var answer = response.responseXML.documentElement.getAttribute("answer");
			if(answer == "true"){
				g_form.showErrorBox("start_datetime","Start datetime must not be within holidays", true);
			}
	}
	
}
]]></script>
