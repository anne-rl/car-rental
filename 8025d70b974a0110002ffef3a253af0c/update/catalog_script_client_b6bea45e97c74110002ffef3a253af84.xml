        <script><![CDATA[function onSubmit() {
  
   //Type appropriate comment here, and begin script below

	var rentprice = new GlideAjax("CarRentalUtils");
	rentprice.addParam("sysparm_name", "rentPriceNotify");
	rentprice.addParam("sysparm_rentprice", g_form.getValue('rent_price'));
	rentprice.addParam("sysparm_startdatetime", g_form.getValue('start_datetime'));
	rentprice.addParam("sysparm_enddatetime", g_form.getValue('end_datetime'));
	
	rentprice.getXML(rentPriceNotify);
	
	function rentPriceNotify(response){
			var answer = response.responseXML.documentElement.getAttribute("answer");
		
			g_form.addInfoMessage(answer);
		
	}
	

}]]></script>
