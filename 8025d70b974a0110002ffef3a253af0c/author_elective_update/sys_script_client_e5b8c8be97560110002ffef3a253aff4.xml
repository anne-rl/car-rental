        <name>Datetime validation for meetings</name>
        <order/>
        <script><![CDATA[function onSubmit() {
	//Type appropriate comment here, and begin script below
	var today = new Date();
	var startDateTime = new Date(g_form.getValue('start_datetime'));
	var endDateTime = new Date(g_form.getValue('end_datetime'));

	var difference = Math.abs((startDateTime.getTime() - endDateTime.getTime()) / 3600000);

	var startDateFormatted = startDateTime.getDate() + "/" + (startDateTime.getMonth() + 1) + "/" + startDateTime.getFullYear();
	var todayFormatted = today.getDate() + "/" + (today.getMonth() + 1) + "/" + today.getFullYear();


	if (endDateTime.getTime() < startDateTime.getTime()) {
		g_form.showErrorBox('end_datetime', 'End Datetime should not be earlier than Start Datetime', false);
		return false;
	}

	if (g_form.isNewRecord() && (startDateTime.getTime() < today.getTime())) {
		g_form.showErrorBox('datetime', 'Start Datetime should not be in the past', false);
		return false;
	}
	
	return true;
}]]></script>
