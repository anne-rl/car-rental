        <isolate_script>true</isolate_script>
        <messages/>
        <name>Set Applicant Name for meetings</name>
        <order/>
        <script><![CDATA[function onLoad() {
  //Check to see if the form is for a new record.  If it is a new record, 
  //set the applicant name to the currently logged in user.

    if(g_form.isNewRecord()){
        g_form.setValue('applicant_name',g_user.userID);
    }
}]]></script>
