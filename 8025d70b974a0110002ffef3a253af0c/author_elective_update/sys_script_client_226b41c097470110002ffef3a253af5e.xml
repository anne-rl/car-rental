        <messages/>
        <name>onChange end business week/hours pf </name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	
//isDateWithinBusinessHours

	if(g_form.getValue("end_datetime") != "" ){
		var today = new Date();

		var end = new Date(g_form.getValue('end_datetime'));
		

		var enddatetime = end.getHours();
		
		
	
	

	}

	if ( enddatetime < 9 ||  enddatetime > 21) {
		g_form.showErrorBox(' end_datetime', 'Start datetime must be within business hours: 9:00 - 21:00');
	}
	

	var enddatetimeweek = end.getDay();
    // Check if saturday or sunday
	if (enddatetimeweek  == 6 || enddatetimeweek  == 0) {
		g_form.showErrorBox('end_datetime', 'End datetime must be within Business Days: Mon-Fri');
		return false;
	}
	
}


	
   
]]></script>
