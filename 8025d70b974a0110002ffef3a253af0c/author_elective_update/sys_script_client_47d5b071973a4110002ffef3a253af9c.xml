        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

	var today = new Date();

	var end = new Date(g_form.getValue('end_datetime'));


	var enddatetime = end.getDay();

	
    // Check if saturday or sunday
	if (enddatetime == 6 || enddatetime == 0) {
		g_form.showErrorBox('end_datetime', 'End datetime must be within Business Days: Mon-Fri');
		return false;
	}

   //Type appropriate comment here, and begin script below
   
}]]></script>
